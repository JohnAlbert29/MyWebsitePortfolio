{
    
  "name": "analytics",
  "version": "1.0.0",
   "scripts": {
    "build": "mkdir -p dist && cp -r *.html *.css *.js images/ netlify/ dist/",
    "postbuild": "find dist -type f -name '*.html' -exec sed -i '' 's/href=\"/href=\"\\//g' {} +",
    "start": "serve dist"
  },
  "dependencies": {
    "@supabase/supabase-js": "^2.0.0",
    "node-fetch": "^2.6.7"
  },
  "devDependencies": {
    "serve": "^14.2.0"
  }
}